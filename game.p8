function _update()
	update_keypad()

	-- read input
	local dx,dy = 0,0
	if btn(0) or keypad(7) then dx -= 1 end
	if btn(1) or keypad(9) then dx += 1 end
	if btn(2) or keypad(5) then dy -= 1 end
	if btn(3) or keypad(8) then dy += 1 end

	if keypadp(11) then
		dx,dy = rnd(8)-rnd(8),rnd(8)-rnd(8)
	end

	-- move
	if dx~=0 or dy~=0 then
		px += dx
		py += dy

		-- stay inside boundaries
		local bx = mid(0,84-5, px)
		if px~=bx then
			sfx(0)
			px = bx
		end
		local by = mid(0,48-5, py)
		if py~=by then
			sfx(0)
			py = by
		end
	end
end

function _draw()
	-- draw your game:
	-- - with colors 1 and 7
	-- - between 0,0 and 83,47 (inclusive)
	cls(1)
	print("\14keypad",1,1,7)
	print("(qweasd etc)")

	-- draw keypad
	for i=1,12 do
		local char = keypad(i) and "█" or "▒"
		local x,y = (i-1)%3,(i-1)\3
		print(char,5+x*9,17+y*7)
	end

	spr(1,px,py)
end

