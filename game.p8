-- nokia template
-- this must be the last tab

_keypad_now,_keypad_press = 0,0
function update_keypad()
	local bits = 0
	-- https://fossies.org/linux/SDL2/include/SDL_scancode.h
	for i,scancode in ipairs({30,31,32,20,26,8,4,22,7,29,27,6}) do
		if stat(28,scancode) then
			bits |= 1<<i-1
		end
	end
	-- arrows
	for i,dir in ipairs({7,9,5,8}) do
		if btn(i-1) then
			bits |= 1<<dir-1
		end
	end
	_keypad_press = bits&~_keypad_now
	_keypad_now = bits
end
function _keypad_helper(bits,n)
	return not n
		and bits
		or bits>>>(n-1)&1==1
end

_camera,camera=camera,function(x,y)
	--align to pause menu
	return _camera(tonum(x or 0)-22,tonum(y or 0)-40)
end

--_real_frame=nil
__draw,_draw=_draw,function()
	-- suppress pause menu -- black is visible
	if btn(6) then poke(0x5f30,1) end

	-- skip every other frame (15fps)
	_real_frame=not _real_frame
	if _real_frame then
		camera()
		__draw()
	end

	-- dev boundary
	-- purple region will not be visible
	camera()
	rect(-1,-1,84,48,2)
end

