version 41
__lua__
-- mycoolgame
-- by mycoolname

-- based on pancelor's template:
-- https://github.com/pancelor/nokiajam-pico8-template

-- player
plx = 42
ply = 24
plr = 3

function _update()
	-- read input
	local dx = tonum(btn(1))-tonum(btn(0))
	local dy = tonum(btn(3))-tonum(btn(2))
	-- move
	plx += dx*2
	ply += dy*2
	-- stay inside boundaries
	plx = mid(plr,83-plr, plx)
	ply = mid(plr,47-plr, ply)
end

function _draw()
	-- nokia3310 stuff
	if(btn(6)) poke(0x5f30,1)
	flip() --15fps
	if(btn(6)) poke(0x5f30,1)
	camera(-22,-40) --align pause menu

	-- draw game; use colors 1 and 7
	cls(1)
	circfill(plx,ply,plr,7)

	-- dev boundary - purple region will not be visible
	rect(-1,-1,84,48,2)
	fillp(â–’)
	rect(-2,-2,85,49,2)
	rect(-3,-3,86,50,2)
	fillp()
end
